@model Fiap.Web.RotaDaReciclagem.ViewModels.Morador.MoradorDetailViewModel

<h1>Morador</h1>

<h4>Detalhes do Morador</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <div class="form-group">
            <label asp-for="MoradorId" class="control-label fw-bold"></label>
            <div class="col-md-10">
                <span>@Model.MoradorId</span>
            </div>
        </div>
        <div class="form-group">
            <label asp-for="Nome" class="control-label fw-bold"></label>
            <div class="col-md-10">
                <span>@Model.Nome</span>
            </div>
        </div>
        <div class="form-group">
            <label asp-for="Endereco" class="control-label fw-bold"></label>
            <div class="col-md-10">
                <span>@Model.Endereco</span>
            </div>
        </div>
        <div class="form-group">
            <label asp-for="EnderecoNumero" class="control-label fw-bold">Nº Endereço</label>
            <div class="col-md-10">
                <span>@Model.EnderecoNumero</span>
            </div>
        </div>
        <div class="form-group">
            <label asp-for="EnderecoComplemento" class="control-label fw-bold">Complemento</label>
            <div class="col-md-10">
                <span>@Model.EnderecoComplemento</span>
            </div>
        </div>
        <div class="form-group">
            <label asp-for="EnderecoBairro" class="control-label fw-bold">Bairro</label>
            <div class="col-md-10">
                <span>@Model.EnderecoBairro</span>
            </div>
        </div>
        <div class="form-group">
            <label asp-for="Email" class="control-label fw-bold"></label>
            <div class="col-md-10">
                <span>@Model.Email</span>
            </div>
        </div>
        <div class="form-group">
            <label asp-for="Telefone" class="control-label fw-bold"></label>
            <div class="col-md-10">
                <span>@Model.Telefone</span>
            </div>
        </div>
        <br />
    </div>
</div>
<div>
    <a asp-action="Index">Voltar</a>
</div>
